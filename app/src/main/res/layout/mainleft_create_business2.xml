        android:fontFamily="@string/PingFang_SC_Medium"
        android:textColorHint="@color/needcar_txt_color1"
        android:paddingRight="@dimen/dp_32"
        android:textSize="@dimen/sp_14"/>
</LinearLayout>
    <View
        style="@style/view_top_bottom10"
        android:layout_marginLeft="@dimen/dp_25"
        android:visibility="gone"
        android:layout_marginRight="@dimen/dp_24"/>
    <LinearLayout
        android:id="@+id/lin_create_business"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginRight="@dimen/dp_13"
            android:paddingTop="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_12"
            android:paddingLeft="@dimen/dp_12"
            android:paddingRight="@dimen/dp_12"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/main_left11"
                android:textColor="@color/needcar_txt_color4"
                android:textSize="@dimen/sp_14"
                android:paddingRight="@dimen/dp_32"
                android:fontFamily="@string/PingFangSC_Regular"
                />
            <TextView
                android:layout_toRightOf="@+id/tv1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/work_item_txt_color"
                android:paddingRight="@dimen/dp_32"
                android:hint="@string/main_left14"
                android:fontFamily="@string/PingFangSC_Regular"
                android:textColorHint="@color/needcar_txt_color1"
                android:layout_toLeftOf="@+id/img_right"
                android:textSize="@dimen/sp_14"/>
            <ImageView
                android:id="@+id/img_right"
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content"
                android:src="@mipmap/icon_right_gray"/>
        </RelativeLayout>
        <View
            android:visibility="gone"
            style="@style/view_top_bottom10"
            android:layout_marginLeft="@dimen/dp_25"
            android:layout_marginRight="@dimen/dp_24"/>
        <RelativeLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginRight="@dimen/dp_13"
            android:paddingTop="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_12"
            android:paddingLeft="@dimen/dp_12"
            android:paddingRight="@dimen/dp_12"
