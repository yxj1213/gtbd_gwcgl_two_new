<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fa_color"
    android:orientation="vertical">

    <include layout="@layout/title_bar"
        android:background="@drawable/titlebar"/>

        <TextView
            android:id="@+id/txt_big"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/main_left9"
            android:textColor="@color/black_color"
            android:textSize="@dimen/sp_26"
            android:layout_marginLeft="@dimen/dp_24"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginBottom="@dimen/dp_28"
            android:fontFamily="@string/PingFangSC_Regular"
            />

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginRight="@dimen/dp_13"
    android:padding="@dimen/dp_12"
    android:orientation="horizontal">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/main_left10"
        android:textColor="@color/needcar_txt_color4"
        android:textSize="@dimen/sp_14"
        android:paddingRight="@dimen/dp_32"
        android:fontFamily="@string/PingFangSC_Regular"
        />
    <EditText
        android:id="@+id/edt_qymc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/work_item_txt_color"
        android:hint="@string/main_left18"
