                android:layout_height="wrap_content"
                android:textColor="@color/work_item_txt_color"
                android:paddingRight="@dimen/dp_32"
                android:hint="@string/main_left14"
                android:fontFamily="@string/PingFangSC_Regular"
                android:textColorHint="@color/needcar_txt_color1"
                android:layout_toLeftOf="@+id/img_right2"
                android:textSize="@dimen/sp_14"/>
            <ImageView
                android:id="@+id/img_right2"
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content"
                android:src="@mipmap/icon_right_gray"/>
        </RelativeLayout>
        <View
            style="@style/view_top_bottom10"
            android:layout_marginLeft="@dimen/dp_25"
            android:layout_marginRight="@dimen/dp_24"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginRight="@dimen/dp_13"
            android:paddingTop="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_12"
            android:paddingLeft="@dimen/dp_12"
            android:paddingRight="@dimen/dp_12"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/main_left13"
                android:textColor="@color/needcar_txt_color4"
                android:textSize="@dimen/sp_14"
                android:paddingRight="@dimen/dp_32"
                android:fontFamily="@string/PingFangSC_Regular"
                />
            <EditText
                android:id="@+id/edt_xm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/work_item_txt_color"
                android:hint="@string/main_left15"
                android:background="@null"
                android:fontFamily="@string/PingFangSC_Regular"
                android:textColorHint="@color/needcar_txt_color1"
                android:paddingRight="@dimen/dp_32"
                android:textSize="@dimen/sp_14"/>
        </LinearLayout>
        <View
            style="@style/view_top_bottom10"
            android:layout_marginLeft="@dimen/dp_25"
            android:layout_marginRight="@dimen/dp_24"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginRight="@dimen/dp_13"
            android:paddingTop="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_12"
            android:paddingLeft="@dimen/dp_12"
            android:paddingRight="@dimen/dp_12"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/main_left19_1"
                android:textColor="@color/needcar_txt_color4"
                android:textSize="@dimen/sp_14"
                android:paddingRight="@dimen/dp_32"
                android:fontFamily="@string/PingFangSC_Regular"
                />
            <EditText
                android:id="@+id/edt_lxdh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/work_item_txt_color"
                android:hint="@string/main_left19"
                android:background="@null"
                android:fontFamily="@string/PingFangSC_Regular"
                android:textColorHint="@color/needcar_txt_color1"
                android:paddingRight="@dimen/dp_32"
                android:inputType="number"
                android:maxLength="11"
                android:textSize="@dimen/sp_14"/>
        </LinearLayout>
        <View
            style="@style/view_top_bottom10"
            android:layout_marginLeft="@dimen/dp_25"
            android:layout_marginRight="@dimen/dp_24"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginRight="@dimen/dp_13"
            android:paddingTop="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_12"
            android:paddingLeft="@dimen/dp_12"
            android:paddingRight="@dimen/dp_12"
